# Uncomment the next line to define a global platform for your project
platform :ios, '14.0'

target 'Instagram' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Instagram
  pod 'Firebase'
  pod 'Firebase/Database'
  pod 'Firebase/Storage'
  pod 'Firebase/Auth'
  pod 'SDWebImage', '~> 5.0'
  pod 'Alamofire', '~> 5.2'
  pod 'AlamofireImage', '~> 4.1'   

end
